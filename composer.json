{
    "name": "lucasgiovanny/laravel-google-ads",
    "description": "Use Laravel Google Ads REST API easy",
    "keywords": [
        "lucasgiovanny",
        "laravel",
        "laravel-google-ads"
    ],
    "homepage": "https://github.com/lucasgiovanny/laravel-google-ads",
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Giovanny",
            "email": "lucasgiovanny@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.3",
        "illuminate/contracts": "^8.37",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "lucasgiovanny\\LaravelGoogleAds\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "lucasgiovanny\\LaravelGoogleAds\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "lucasgiovanny\\LaravelGoogleAds\\LaravelGoogleAdsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
